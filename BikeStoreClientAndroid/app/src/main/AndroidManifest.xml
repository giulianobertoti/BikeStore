<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thiagofortunato.androidlojedebike" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AdicionarBike"
            android:label="@string/title_activity_adicionar_bike" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DeletarBike"
            android:label="@string/title_activity_deletar_bike" >
        </activity>
        <activity
            android:name=".ConsultarBike"
            android:label="@string/title_activity_consultar_bike" >
        </activity>
        <activity
            android:name=".AlterarBike"
            android:label="@string/title_activity_alterar_bike" >
        </activity>
    </application>

</manifest>
